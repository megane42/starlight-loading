{
    "name": "Starlight Loading",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "Starlight Stage Load Screen Emulator",
    "homepage_url": "http://starlight.tsudanuma-studio.net/",
    "icons": {
        "16": "data/icon16.png",
        "48": "data/icon48.png",
        "128": "data/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js" : ["data/pace-option.js", "data/pace.min.js", "data/starlight-loading.js"],
            "css": ["data/pace.css", "data/mplus-1p-bold-sub-woff.css", "data/starlight-loading.css"],
            "run_at": "document_start"
        }
    ]
}
